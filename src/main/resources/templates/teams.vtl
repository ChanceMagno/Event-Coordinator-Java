<!--teams.vtl-->
<h1>Team Name: $team.getTeamName()</h1>
  #if($team.getMembers())
    <ul>
      #foreach ($member in $team.getMembers())
      <li>$member.getName()</li>
      #end
    </ul>
  #else
    <p>There are currently no members.</p>
  #end
<a href="/teams/$team.getId()/member/new">Add a new member</a>
<a href="/">Back to all teams</a>
<!--teams.vtl-->
